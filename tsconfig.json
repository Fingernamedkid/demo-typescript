{
  "compilerOptions": {
                                     
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "esnext",                   
    "sourceMap": true,                               
    "outDir": "./dist",                              
    "esModuleInterop": true,                         
    "strict": true,                                  
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true
                            
  }
,"exclude": ["node_modules"]

}
